{
    "extends" : "eslint:recommended",
    "plugins" : [
        "react"
    ],
    "env": {
        "browser": true,
        "commonjs": true,
        "node": true,
        "es6": true
    },
    "parser":"babel-eslint",
    "rules": {
        "indent": [2, 4, {"indentSwitchCase": true}],
        "semi" : ["error", "always"],
        "no-console": "off",
        "react/jsx-uses-react":"error",
        "react/jsx-uses-vars":"error"
    },
    "globals": {
        "console": false
    }
}